# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
name = Jesse Lumarie
	email = jesse.lumarie@gmail.com

[color]
  branch = auto
  diff = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = green
  changed = yellow
  untracked = cyan

[alias]
	co = checkout
	last = log -1 HEAD
	sync = !git fetch origin && git rebase origin/master
	mine = !git log --no-merges --author=$(git config user.email)
[push]
	default = current
[core]
	excludesfile = /Users/jlumarie/.gitignore_global
[url "git@github.com:"]
	insteadOf = https://github.com/
[commit]
	template = /Users/jlumarie/.git-commit-msg
